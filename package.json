{
  "scripts": {
    "lint:run": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "tweet:today:vaccination": "pip install --upgrade pip && pip install tweepy && python ./src/tweet/today/Vaccination.py",
    "tweet:today:case": "pip install --upgrade pip && pip install tweepy && python ./src/tweet/today/Case.py",
    "tweet:today:all": "npm run tweet:today:case && npm run tweet:today:vaccination",
    "tweet:all": "npm run tweet:today:all",
    "slack:daily:vaccination": "ts-node ./src/slack/daily/Vaccination",
    "slack:daily:all": "npm run slack:daily:vaccination",
    "slack:today:case": "ts-node ./src/slack/today/Case",
    "slack:today:vaccination": "ts-node ./src/slack/today/Vaccination",
    "slack:today:all": "npm run slack:today:case && npm run slack:today:case",
    "slack:all": "npm run slack:today:all && npm run slack:daily:all",
    "badge:vaccination": "ts-node ./src/badge/Vaccination.ts",
    "badge:case": "ts-node ./src/badge/Case.ts",
    "badge:all": "npm run badge:case && npm run badge:vaccination",
    "plain:vaccination": "ts-node ./src/plain/Vaccination.ts",
    "plain:case": "ts-node ./src/plain/Case.ts",
    "plain:all": "npm run plain:case && npm run plain:vaccination",
    "fetch:vaccination": "ts-node ./run/fetch/Vaccination.ts",
    "fetch:case": "ts-node ./run/fetch/Case.ts",
    "fetch:all": "npm run fetch:case && npm run fetch:vaccination"
  },
  "dependencies": {
    "@corcc/request": "github:corcc/request#dist",
    "@slack/events-api": "^3.0.1",
    "@slack/web-api": "^6.6.0",
    "@taccl/thousands": "https://github.com/taccl/thousands.git",
    "@types/html-parser": "^0.11.2",
    "@types/twitter": "^1.7.1",
    "@types/twitter-text": "^3.1.1",
    "@types/xml2js": "^0.4.9",
    "badgen": "^3.2.2",
    "node-html-parser": "^5.3.3",
    "ts-node": "^10.4.0",
    "twitter": "^1.7.1",
    "twitter-text": "^3.1.0",
    "typescript": "^4.5.5",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0"
  }
}